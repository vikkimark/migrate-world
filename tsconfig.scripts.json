{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": ".out",
    "esModuleInterop": true,
    "skipLibCheck": true
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["scripts/**/*.ts"]
}
